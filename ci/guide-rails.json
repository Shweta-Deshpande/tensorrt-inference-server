{
	"metadata": {
		"version": 1
	},
	"application": {
		"name": "tensorrt-inference-server",
		"language": "CPP",
		"artifact_store": {
			"type": "s3",
			"repositories": [
				{
					"category": "rc-packaging",
					"bucket": "guide-rails-calculi-rc"
				},
				{
					"category": "final-packaging",
					"bucket": "guide-rails-calculi-final"
				},
				{
					"category": "logs",
					"bucket": "guide-rails-calculi-logs"
				}
			]
		}
	},
	"pipeline": {
		"segments": [
			{
				"name": "Build",
				"segment_type": "build",
				"job_steps": [
					{
						"name": "Print Env",
						"definition": "Print Environment",
						"command": "printenv | sort",
						"properties": null
					},
					{
						"name": "Configure",
						"definition": "Configure the build",
						"command": "cmake -G 'Unix Makefiles'",
						"properties": null
					},
					{
						"name": "Install",
						"definition": "Make Install",
						"command": "cd build\n make install",
						"properties": null
					},
					{
						"name": "Test",
						"definition": "Make Test",
						"command": "cd build\nexport GTEST_OUTPUT=\\'xml:./test.xml'\nmake test\nfind . -name \\'*.gcda\\' \nls ./test/results'",
						"properties": null
					},
					{
						"name": "Merge Tests",
						"definition": "Merge Test Results",
						"command": "python ./xmlcombine.py ./build/test/results/*.xml  \u003e ./test.xml\nls -l",
						"properties": null
					},
					{
						"name": "CPP Check",
						"definition": "Perform CPP Check",
						"command": "cppcheck --xml --enable=all -I include src/* 2\u003e report.xml",
						"properties": null
					},
					{
						"name": "Coverage Report",
						"definition": "Generate Coverage Report",
						"command": "list=\\'$(find . -name *.gcda)\\'\nfor file in $list; do gcov \\'$file\\' ; done\ngcovr --root  . --xml  --output ./coverage.xml",
						"properties": null
					}
				],
				"exit_actions": {
					"default": {
						"fetch_logs": true,
						"code_analysis": {
							"publish": true,
							"properties": []
						},
						"delete_instances": true
					},
					"on_success": {
						"code_analysis": null
					},
					"on_failure": {
						"code_analysis": null
					},
					"on_abort": {
						"code_analysis": null
					}
				},
				"deployment_strategy": {},
				"application_init": {
					"command": "",
					"arguments": null
				},
				"packages": [
					{
						"name": "((application.name))",
						"dependencies": [],
						"downloads": [],
						"files": [
							"((application.name))*"
						],
						"package_install_command": "",
						"installed_path": ""
					}
				],
				"instance_tags": [
					{
						"name": "application.name",
						"value": "((application.name))"
					}
				],
				"post_deployment_actions": {
					"instances": null,
					"loadbalanced": false,
					"loadbalancer": {
						"TargetGroupName": "",
						"TargetGroupPort": 0,
						"TargetGroupProtocol": "",
						"TargetType": "",
						"VPCID": "",
						"HealthCheckEnabled": false,
						"HealthCheckPort": "",
						"UnhealthyThresholdCount": 0
					}
				},
				"cache_info": {}
			},
			{
				"name": "Production",
				"segment_type": "production",
				"persistent": true,
				"run_after": [
					"Build"
				],
				"exit_actions": {
					"default": {
						"fetch_logs": true,
						"code_analysis": {
							"publish": true,
							"properties": []
						},
						"delete_instances": false
					},
					"on_success": {
						"code_analysis": null,
						"delete_instances": false
					},
					"on_failure": {
						"code_analysis": null,
						"delete_instances": false
					},
					"on_abort": {
						"code_analysis": null,
						"delete_instances": false
					}
				},
				"deployment_strategy": {
					"canaries": 1,
					"max_in_flight": 1,
					"canary_watch_time": "5000-30000",
					"update_watch_time": "5000-30000",
					"serial": false
				},
				"application_init": {
					"command": "",
					"arguments": null
				},
				"process": {
					"init": {
						"command": ""
					}
				},
				"package_dependencies": [
					"((application.name))"
				],
				"instance_tags": [
					{
						"name": "application.name",
						"value": "((application.name))"
					}
				],
				"instance_groups": [
					{
						"name": "((application.name))",
						"number_of_instances": 1,
						"persistent_disk_type": "",
						"vm_type": "default",
						"os_type": "default",
						"networks": [
							"((deploy.target.network.production))"
						],
						"availability_zones": [
							"((deploy.target.zone.production))"
						],
						"deployment_strategy": {},
						"dependencies": [
							{
								"name": "consul",
								"release": "consul",
								"provides": [
									{
										"name": "consul_servers.as",
										"value": "consul_1"
									}
								],
								"consumes": [
									{
										"name": "consul_servers.from",
										"value": "consul_1"
									}
								],
								"properties": [
									{
										"name": "consul.servicename",
										"value": "((consul.servicename))"
									},
									{
										"name": "consul.environment",
										"value": "((consul.environment))"
									},
									{
										"name": "consul.datacenter",
										"value": "((consul.datacenter))"
									},
									{
										"name": "consul.server",
										"value": "false"
									},
									{
										"name": "consul.joinservers",
										"value": "((consul.joinservers))"
									},
									{
										"name": "calculi.intermediate.cert",
										"value": "grsecret:tls.cert"
									},
									{
										"name": "calculi.intermediate.key",
										"value": "grsecret:tls.key"
									}
								]
							}
						]
					}
				],
				"post_deployment_actions": {
					"instances": null,
					"loadbalanced": false,
					"loadbalancer": {
						"TargetGroupName": "",
						"TargetGroupPort": 0,
						"TargetGroupProtocol": "",
						"TargetType": "",
						"VPCID": "",
						"HealthCheckEnabled": false,
						"HealthCheckPort": "",
						"UnhealthyThresholdCount": 0
					}
				}
			}
		],
		"exit_actions": {},
		"default_properties": null,
		"deployment_files": [],
		"application_init": {
			"command": "",
			"arguments": null
		},
		"process": {
			"init": {
				"command": "bin/start-service.sh"
			}
		},
		"releases": [
			{
				"name": "consul",
				"version": "latest"
			}
		]
	},
	"organization": {
		"orgname": "Shweta-Deshpande"
	}
}